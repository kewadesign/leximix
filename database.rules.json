{
  "rules": {
    ".read": true,
    ".write": true,
    "system": {
      ".read": true,
      ".write": false
    },
    "vouchers": {
      ".read": true,
      ".write": false,
      "$voucherId": {
        "usedBy": {
          "$username": {
            ".write": "!data.exists()"
          }
        }
      }
    }
  }
}
